{
    "workbench.colorTheme": "Oceanic Plus",
    "git.confirmSync": false,
    "python.linting.pylintEnabled": true,
    "workbench.colorCustomizations": {
        "peekViewEditor.matchHighlightBackground": "#e2934991",
        "[One Dark Pro]": {
            "editor.selectionHighlightBorder": "#333b3f",
            "editor.selectionHighlightBackground": "#333b3f",
            "editor.background": "#282C34"
        },
        "[Atom One Dark]": {
            "activityBar.background": "#21252b"
        },
        "[Oceanic Next]": {
            "statusBar.background": "#252526",
            "titleBar.activeBackground": "#252526",
            "activityBar.background": "#252526"
        }
    },
    "[javascript]": {
        "editor.tabSize": 2,
        "editor.formatOnSave": true
    },
    "[typescript]": {
        "editor.tabSize": 2,
        "editor.formatOnSave": true
    },
    "[typescriptreact]": {
        "editor.tabSize": 2,
        "editor.formatOnSave": true
    },
    "[go]": {
        "editor.formatOnSave": true
    },
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/**": true
    },
    "workbench.editor.enablePreview": false,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "explorer.openEditors.visible": 0,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "python.formatting.provider": "black",
    "python.pythonPath": "~/.virtualenvs/default/bin/python",
    "python.venvPath": "~/.virtualenvs",
    "editor.formatOnSave": false,
    "gitlens.codeLens.enabled": false,
    "gitlens.mode.statusBar.enabled": false,
    "gitlens.statusBar.enabled": false,
    "breadcrumbs.enabled": true,
    "git.autofetch": true,
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "files.associations": {
        "*mdx": "markdown"
    },
    "liveshare.featureSet": "insiders",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter.notebook.ipynb"
    },
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "typescript.updateImportsOnFileMove.enabled": "always",
    "python.defaultInterpreterPath": "~/.virtualenvs/default/bin/python",
    "editor.detectIndentation": false,
    "editor.unicodeHighlight.ambiguousCharacters": false,
    "editor.inlineSuggest.enabled": true,
    "[python]": {
        "editor.formatOnType": true
    }
}
